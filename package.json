{
  "name": "steamer-vue-component",
  "version": "3.0.0-beta.1",
  "description": "vue component development starter kit",
  "main": "dist/index.js",
  "scripts": {
    "start": "node ./tools/script.js --mode=development",
    "dev": "node ./tools/script.js --mode=development",
    "source": "node ./tools/script.js --mode=source",
    "dist": "npm run babel",
    "test": "npm run karma",
    "babel": "babel src --out-dir dist --copy-files",
    "webpack": "node ./tools/script.js --mode=production",
    "karma": "cross-env BABEL_ENV=test node ./tools/script.js --mode=karma",
    "lint": "eslint src/**/*.js && stylelint src/**/*.less",
    "prepublish": "npm run dist"
  },
